{
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "frontendDist": "../ui"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "android": {
      "minSdkVersion": 29
    },
    "resources": [
      "resources/*"
    ],
    "longDescription": "",
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "bookmark",
  "mainBinaryName": "bookmark",
  "version": "0.3.0",
  "identifier": "com.bookmark.dev",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "height": 800,
        "resizable": true,
        "title": "bookmark",
        "width": 400,
        "useHttpsScheme": true
      },
      {
        "label": "bookDetails",
        "create": false,
        "url": "bookDetails.html",
        "height": 800,
        "width": 400,
        "x": 200,
        "y": 200,
        "resizable": false,
        "title": "bookDetails"
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": {
          "requireLiteralLeadingDot": false,
          "allow": [
            "**/*"
          ]
        }
      }
    }
  }
}